    <style name="Theme.AndroidStartup.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.AndroidStartup.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>